<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="proyecto.seguros">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Principal">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PrincipalPersona"
            android:label="@string/title_activity_principal_persona"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PrincipalPoliza"
            android:label="@string/title_activity_principal_poliza"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Reportes"></activity>
    </application>

</manifest>